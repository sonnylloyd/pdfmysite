{% extends "base.html.twig" %}
{% block stylesheets %}
    {% stylesheets 
		'common/libs/bootstrap/css/bootstrap.css'
		'common/css/main.css'
		output='mainfront.css'
		filter='cssrewrite'
                filter='yui_css'
    %} 
    {% endstylesheets %}
    <link rel="stylesheet" href="{{ asset('mainfront.css') }}" />
{% endblock %}
{% block navigation %}
    <nav class="navbar navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
            <a href="{{ path('pdf_my_site_front_homepage') }}">
                <object data="{{ asset('common/img/logo/pdfmysite.svg') }}" type="image/svg+xml">
                    <img src="{{ asset('common/img/logo/pdfmysite.png') }}" alt="pdfmysite logo" />
                </object>
            </a>
        </div>
        <div id="navbar" class="collapse navbar-collapse">
          <ul class="nav navbar-nav navbar-right">
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#about">Archives</a></li>
            <li><a href="#contact">Contact</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>	
{% endblock %}
{% block footer %}
{% endblock %}
{% block javascript %}
    {% javascripts
		'common/libs/jquery/js/jquery-1.11.1.min.js'
		'common/libs/bootstrap/js/bootstrap.js'
		output='mainfront.js'
                filter='yui_js'
    %}
    {% endjavascripts %}
    <script src="{{ asset('mainfront.js') }}"></script>
{% endblock %}
{% block headscript %}
    <!--[if lt IE]>
    {% javascripts
		'common/libs/bootstrap/js/ie-emulation-modes-warning.js'
                'common/libs/bootstrap/js/ie10-viewport-bug-workaround.js'
		'https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js'
		'https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js'
		output='mainfront.ie.js'
                filter='yui_js'
    %}
    {% endjavascripts %}
    <script src="{{ asset('mainfront.ie.js') }}"></script>
    <![endif]-->
{% endblock %}